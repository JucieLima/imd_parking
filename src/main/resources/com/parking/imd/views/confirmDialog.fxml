<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>


<AnchorPane prefHeight="238.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.parking.imd.controllers.ConfirmDialogController">
   <children>
      <Label alignment="CENTER" layoutX="155.0" layoutY="26.0" text="Confirmação" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextFlow layoutX="26.0" layoutY="61.0" prefHeight="114.0" prefWidth="326.0">
         <children>
            <Text fx:id="textConfirm" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirma esta ação?">
               <font>
                  <Font size="16.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
      <Button layoutX="261.0" layoutY="178.0" mnemonicParsing="false" onAction="#handleButtonConfirm" text="Confirmar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="168.0" layoutY="178.0" mnemonicParsing="false" onAction="#handleButtonCancel" text="Cancelar">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
